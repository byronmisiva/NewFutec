                    }
                } else {
                    document.getElementById("div-gpt-ad-1383593619381-0").style.display = 'block';
                }
            }
        });


        googletag.enableServices();

        //para el caso que no existe publicicad --MISIVA--
        googletag.pubads().collapseEmptyDivs(true);

    });

